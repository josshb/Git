package control;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Elemento E1=new Elemento();
		Scanner teclado=new Scanner(System.in);
		System.out.println("Introduce el nombre de tu elemento");
		E1.setName(teclado.nextLine());
		System.out.println("Introduce el simbolo de tu elemento");
		E1.setSymbol(teclado.nextLine());

		System.out.println("Introduce el numero atomico de tu elemento");
		E1.setNumero(teclado.nextInt());
		
		System.out.println("esta es la ficha de tu elemento:\n"+E1);
	}

}
