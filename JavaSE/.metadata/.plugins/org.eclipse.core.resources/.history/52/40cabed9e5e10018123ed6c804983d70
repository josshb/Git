package control;

import java.util.Arrays;

import utilidades.LeerDatos;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int tamañoarray = 5;
		int suma = 0;
		int[] array1 = new int[tamañoarray];
		for (int i = 0; i < array1.length; i++) {
			System.out.print("Introduce el primer dato entero para nuestro array");
			array1[i] = LeerDatos.leerint();
			suma += array1[i];
		}
		StringBuilder alverres = new StringBuilder((Arrays.toString(array1)));
		/*System.out.println(Arrays.toString(array1));
		System.out.println("suma " + suma);*/
		System.out.println(alverres.reverse());
	}

}