package utilidades;

public class ContadorArrays {

	
	
	private int tamanoArray;
	public ContadorArrays(){
		
	}
	
	public ContadorArrays(int tamanoArray) {
		super();
		this.tamanoArray = tamanoArray;
	}

	public int getTamanoArray() {
		return tamanoArray;
	}

	public void setTamanoArray(int tamanoArray) {
		this.tamanoArray = tamanoArray;
	}
	

}
